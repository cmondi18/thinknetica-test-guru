<tr>
  <td style="text-align: center">
    <%= content_tag :span,
                    test.title,
                    class: 'test-title form-inline-title',
                    data: { test_id: test.id } %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td style="text-align: center"><%= test.level %></td>
  <td style="text-align: center"><%= test.questions.count %></td>
  <td style="text-align: center"><%= test.public %></td>
  <td style="text-align: center"><%= link_to t('.show'), admin_test_path(test) %></td>
  <td style="text-align: center"><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td style="text-align: center"><%= link_to t('.delete'), admin_test_path(test), method: :delete, data: { confirm: t('.confirm') } %></td>
</tr>