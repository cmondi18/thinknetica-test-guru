<h1><%= t('.header', title: @test.title) %></h1>
<% if @test.questions.present? %>
  <table class="table table-hover">
    <tr>
      <th><%= t('.questions') %></th>
      <th colspan="3"><%= t('.actions') %></th>
    </tr>
    <%= render @questions %>
  </table>
<% else %>
  <p><%= t('.no_questions') %></p>
<% end %>
<p>
  <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
</p>
<p>
  <%= link_to t('.edit'), edit_admin_test_path(@test) %>
</p>
<p>
  <%= link_to t('.back_to_tests'), admin_tests_path %>
</p>