<h1><%= t('.header') %> </h1>

<% if @tests.present? %>
  <table>
    <tr>
      <th><%= t('.title') %></th>
      <th><%= t('.level') %></th>
      <th><%= t('.questions_count') %></th>
      <th colspan><%= t('.actions') %></th>
    </tr>
    <%= render @tests %>
  </table>
<% else %>
  <h1><%= t('.no_tests') %></h1>
<% end %>
